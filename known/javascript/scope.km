                            {
                                "data": {
                                    "id": 0,
                                    "created": 1641538225192,
                                    "text": "传统编译语言编译的3个步骤",
                                    "layout": null
                                },
                                "children": [
                                    {
                                        "data": {
                                            "id": 0,
                                            "created": 1641538240832,
                                            "text": "1: 分词/词法分析",
                                            "layout": null
                                        },
                                        "children": [
                                            {
                                                "data": {
                                                    "id": 0,
                                                    "created": 1641538327837,
                                                    "text": "将代码字符串分解为有意义的代码块（词法单元）",
                                                    "layout": null
                                                },
                                                "children": []
                                            },
                                            {
                                                "data": {
                                                    "id": 0,
                                                    "created": 1641538414402,
                                                    "text": "例如把:var a = 2;分解为：var、a、=、 2、 ;",
